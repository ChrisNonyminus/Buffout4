{
  "name": "buffout4",
  "version-string": "1",
  "description": "A crash reporting and bug fixing plugin for Fallout 4",
  "homepage": "https://github.com/Ryan-rsm-McKenzie/Buffout4",
  "license": "MIT",
  "dependencies": [
    "boost-algorithm",
    "boost-atomic",
    "boost-container",
    "boost-iostreams",
    "boost-iterator",
    "boost-nowide",
    "boost-predef",
    "boost-stacktrace",
    "boost-stl-interfaces",
    "botan",
    "fmt",
    "frozen",
    {
      "name": "infoware",
      "features": [
        "d3d"
      ]
    },
    "robin-hood-hashing",
    "span-lite",
    "spdlog",
    "tbb",
    "tomlplusplus",
    "xbyak"
  ]
}
